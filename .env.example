# Jira Configuration
JIRA_HOST=your_jira_host
JIRA_USERNAME=your_jira_username
JIRA_API_TOKEN=your_jira_api_token

# GitHub Configuration
GITHUB_TOKEN=your_github_token
GITHUB_OWNER=your_github_username_or_org
